{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  name: 'Agent',\n  data: () => ({\n    valid: true,\n    loading: false,\n    shareDialog: false,\n    message: {\n      name: '',\n      phone: '',\n      email: '',\n      content: ''\n    },\n    nameRules: [v => !!v || 'Name is required'],\n    phoneRules: [v => !!v || 'Phone number is required'],\n    emailRules: [v => !!v || 'Email is required', v => /.+@.+\\..+/.test(v) || 'Email must be valid'],\n    messageRules: [v => !!v || 'Message is required']\n  }),\n  computed: {\n    ...mapGetters(['isAuthenticated', 'currentUser'])\n  },\n  methods: {\n    async submitMessage() {\n      if (this.$refs.form.validate()) {\n        this.loading = true;\n        try {\n          // 这里添加发送消息的逻辑\n          await new Promise(resolve => setTimeout(resolve, 1000));\n          this.$refs.form.reset();\n          alert('Message sent successfully!');\n        } catch (error) {\n          console.error('Error sending message:', error);\n          alert('Failed to send message. Please try again.');\n        } finally {\n          this.loading = false;\n        }\n      }\n    },\n    generateVCard() {\n      if (!this.currentUser) return '';\n      return `BEGIN:VCARD\nVERSION:3.0\nFN:${this.currentUser.name}\nTITLE:${this.currentUser.title}\nTEL;TYPE=WORK,VOICE:${this.currentUser.phone}\nTEL;TYPE=WORK,WHATSAPP:${this.currentUser.whatsapp}\nEMAIL;TYPE=WORK:${this.currentUser.email}\nADR;TYPE=WORK:;;${this.currentUser.address}\nORG:Andy Casket Service\nURL:${window.location.origin}/agent\nEND:VCARD`;\n    },\n    async downloadVCard() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      const vcard = this.generateVCard();\n      const blob = new Blob([vcard], {\n        type: 'text/vcard'\n      });\n      const url = window.URL.createObjectURL(blob);\n      const link = document.createElement('a');\n      link.href = url;\n      link.setAttribute('download', `${this.currentUser.name}.vcf`);\n      document.body.appendChild(link);\n      link.click();\n      document.body.removeChild(link);\n    },\n    shareOnWhatsApp() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      const text = `Contact Card - ${this.currentUser.name}\\n${window.location.href}`;\n      window.open(`https://wa.me/?text=${encodeURIComponent(text)}`);\n    },\n    shareCard() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      this.shareDialog = true;\n    },\n    async copyLink() {\n      try {\n        await navigator.clipboard.writeText(window.location.href);\n        alert('Link copied to clipboard!');\n        this.shareDialog = false;\n      } catch (err) {\n        console.error('Failed to copy:', err);\n      }\n    },\n    shareByEmail() {\n      const subject = `Contact Card - ${this.currentUser.name}`;\n      const body = `Here's my contact information:\\n\\n${window.location.href}`;\n      window.location.href = `mailto:?subject=${encodeURIComponent(subject)}&body=${encodeURIComponent(body)}`;\n      this.shareDialog = false;\n    }\n  },\n  created() {\n    // 检查是否有保存的用户信息\n    const savedUser = localStorage.getItem('user');\n    if (savedUser && !this.isAuthenticated) {\n      this.$store.commit('SET_USER', JSON.parse(savedUser));\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","name","data","valid","loading","shareDialog","message","phone","email","content","nameRules","v","phoneRules","emailRules","test","messageRules","computed","methods","submitMessage","$refs","form","validate","Promise","resolve","setTimeout","reset","alert","error","console","generateVCard","currentUser","title","whatsapp","address","window","location","origin","downloadVCard","isAuthenticated","$router","push","vcard","blob","Blob","type","url","URL","createObjectURL","link","document","createElement","href","setAttribute","body","appendChild","click","removeChild","shareOnWhatsApp","text","open","encodeURIComponent","shareCard","copyLink","navigator","clipboard","writeText","err","shareByEmail","subject","created","savedUser","localStorage","getItem","$store","commit","JSON","parse"],"sources":["src/views/Agent.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 名片页面标题区 -->\n    <v-parallax\n      :src=\"require('@/assets/background.jpg') || 'https://picsum.photos/1920/1080?random=3'\"\n      height=\"300\"\n      class=\"page-header\"\n    >\n      <div class=\"overlay\"></div>\n      <v-container class=\"fill-height\">\n        <v-row align=\"center\" justify=\"center\">\n          <v-col cols=\"12\" md=\"8\" class=\"text-center\">\n            <h1 class=\"text-h2 font-weight-bold white--text mb-4\">Contact Us</h1>\n            <p class=\"text-h6 white--text\">Available 24/7 for your needs</p>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-parallax>\n\n    <v-container class=\"namecard-container mt-n10\">\n      <v-row justify=\"center\">\n        <v-col cols=\"12\" md=\"10\">\n          <!-- 未登录状态显示登录提示 -->\n          <v-card v-if=\"!isAuthenticated\" class=\"namecard-wrapper\" elevation=\"10\">\n            <v-row no-gutters>\n              <v-col cols=\"12\" md=\"4\" class=\"primary\">\n                <div class=\"profile-section pa-6 white--text\">\n                  <v-avatar size=\"180\" class=\"mb-6 profile-avatar\">\n                    <v-icon size=\"120\" color=\"white\">mdi-account-circle</v-icon>\n                  </v-avatar>\n                  <h2 class=\"text-h4 font-weight-bold mb-2\">Agent Profile</h2>\n                  <p class=\"text-h6 mb-6\">Login to view</p>\n                </div>\n              </v-col>\n\n              <v-col cols=\"12\" md=\"8\">\n                <div class=\"login-prompt pa-6\">\n                  <h3 class=\"text-h5 mb-6\">Login to view agent details</h3>\n                  <p class=\"mb-6\">Please login or register to view agent contact information and create your own e-namecard.</p>\n                  <v-btn\n                    color=\"primary\"\n                    large\n                    to=\"/login\"\n                    class=\"mr-4\"\n                  >\n                    <v-icon left>mdi-login</v-icon>\n                    Login\n                  </v-btn>\n                  <v-btn\n                    color=\"accent\"\n                    large\n                    to=\"/login?tab=1\"\n                    outlined\n                  >\n                    <v-icon left>mdi-account-plus</v-icon>\n                    Register\n                  </v-btn>\n                </div>\n              </v-col>\n            </v-row>\n          </v-card>\n\n          <!-- 已登录状态显示名片 -->\n          <v-card v-else class=\"namecard-wrapper\" elevation=\"10\">\n            <v-row no-gutters>\n              <!-- 左侧个人信息 -->\n              <v-col cols=\"12\" md=\"4\" class=\"primary\">\n                <div class=\"profile-section pa-6 white--text\">\n                  <v-avatar size=\"180\" class=\"mb-6 profile-avatar\">\n                    <v-icon size=\"120\" color=\"white\">mdi-account-circle</v-icon>\n                  </v-avatar>\n                  <h2 class=\"text-h4 font-weight-bold mb-2\">{{ currentUser.name }}</h2>\n                  <p class=\"text-h6 mb-6\">{{ currentUser.title }}</p>\n                  <v-divider dark class=\"mb-6\"></v-divider>\n                  <div class=\"contact-info\">\n                    <div class=\"mb-4\">\n                      <v-icon dark class=\"mr-3\">mdi-phone</v-icon>\n                      <span>{{ currentUser.phone }}</span>\n                    </div>\n                    <div class=\"mb-4\">\n                      <v-icon dark class=\"mr-3\">mdi-whatsapp</v-icon>\n                      <span>{{ currentUser.whatsapp }}</span>\n                    </div>\n                    <div class=\"mb-4\">\n                      <v-icon dark class=\"mr-3\">mdi-email</v-icon>\n                      <span>{{ currentUser.email }}</span>\n                    </div>\n                    <div class=\"mb-4\">\n                      <v-icon dark class=\"mr-3\">mdi-map-marker</v-icon>\n                      <span>{{ currentUser.address }}</span>\n                    </div>\n                  </div>\n                </div>\n              </v-col>\n\n              <!-- 右侧联系表单 -->\n              <v-col cols=\"12\" md=\"8\">\n                <div class=\"contact-form pa-6\">\n                  <h3 class=\"text-h5 mb-6\">Send us a message</h3>\n                  <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                    <v-row>\n                      <v-col cols=\"12\" md=\"6\">\n                        <v-text-field\n                          v-model=\"message.name\"\n                          label=\"Your Name\"\n                          :rules=\"nameRules\"\n                          outlined\n                          dense\n                        ></v-text-field>\n                      </v-col>\n                      <v-col cols=\"12\" md=\"6\">\n                        <v-text-field\n                          v-model=\"message.phone\"\n                          label=\"Phone Number\"\n                          :rules=\"phoneRules\"\n                          outlined\n                          dense\n                        ></v-text-field>\n                      </v-col>\n                    </v-row>\n                    <v-text-field\n                      v-model=\"message.email\"\n                      label=\"Email Address\"\n                      :rules=\"emailRules\"\n                      outlined\n                      dense\n                    ></v-text-field>\n                    <v-textarea\n                      v-model=\"message.content\"\n                      label=\"Message\"\n                      :rules=\"messageRules\"\n                      outlined\n                      rows=\"4\"\n                    ></v-textarea>\n                    <v-btn\n                      color=\"primary\"\n                      :loading=\"loading\"\n                      :disabled=\"!valid\"\n                      @click=\"submitMessage\"\n                      x-large\n                      block\n                      class=\"mt-4\"\n                    >\n                      Send Message\n                    </v-btn>\n                  </v-form>\n                </div>\n              </v-col>\n            </v-row>\n\n            <!-- 分享按钮组 -->\n            <v-card-actions class=\"share-actions pa-6 mt-4\">\n              <v-row justify=\"center\" align=\"center\">\n                <v-col cols=\"auto\">\n                  <v-btn\n                    color=\"primary\"\n                    class=\"share-btn mx-2\"\n                    @click=\"downloadVCard\"\n                    elevation=\"2\"\n                  >\n                    <v-icon left>mdi-card-account-details</v-icon>\n                    Save Contact\n                  </v-btn>\n                </v-col>\n                <v-col cols=\"auto\">\n                  <v-btn\n                    color=\"success\"\n                    class=\"share-btn mx-2\"\n                    @click=\"shareOnWhatsApp\"\n                    elevation=\"2\"\n                  >\n                    <v-icon left>mdi-whatsapp</v-icon>\n                    WhatsApp\n                  </v-btn>\n                </v-col>\n                <v-col cols=\"auto\">\n                  <v-btn\n                    color=\"accent\"\n                    class=\"share-btn mx-2\"\n                    @click=\"shareCard\"\n                    elevation=\"2\"\n                  >\n                    <v-icon left>mdi-share-variant</v-icon>\n                    Share\n                  </v-btn>\n                </v-col>\n              </v-row>\n            </v-card-actions>\n          </v-card>\n        </v-col>\n      </v-row>\n\n      <!-- 分享对话框 -->\n      <v-dialog v-model=\"shareDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title class=\"headline\">Share Contact Card</v-card-title>\n          <v-card-text>\n            <v-list>\n              <v-list-item @click=\"copyLink\">\n                <v-list-item-icon>\n                  <v-icon>mdi-content-copy</v-icon>\n                </v-list-item-icon>\n                <v-list-item-content>\n                  <v-list-item-title>Copy Direct Link</v-list-item-title>\n                </v-list-item-content>\n              </v-list-item>\n              <v-list-item @click=\"shareByEmail\">\n                <v-list-item-icon>\n                  <v-icon>mdi-email</v-icon>\n                </v-list-item-icon>\n                <v-list-item-content>\n                  <v-list-item-title>Share via Email</v-list-item-title>\n                </v-list-item-content>\n              </v-list-item>\n            </v-list>\n          </v-card-text>\n        </v-card>\n      </v-dialog>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\nexport default {\n  name: 'Agent',\n  data: () => ({\n    valid: true,\n    loading: false,\n    shareDialog: false,\n    message: {\n      name: '',\n      phone: '',\n      email: '',\n      content: ''\n    },\n    nameRules: [v => !!v || 'Name is required'],\n    phoneRules: [v => !!v || 'Phone number is required'],\n    emailRules: [\n      v => !!v || 'Email is required',\n      v => /.+@.+\\..+/.test(v) || 'Email must be valid'\n    ],\n    messageRules: [v => !!v || 'Message is required'],\n  }),\n  computed: {\n    ...mapGetters(['isAuthenticated', 'currentUser'])\n  },\n  methods: {\n    async submitMessage() {\n      if (this.$refs.form.validate()) {\n        this.loading = true;\n        try {\n          // 这里添加发送消息的逻辑\n          await new Promise(resolve => setTimeout(resolve, 1000));\n          this.$refs.form.reset();\n          alert('Message sent successfully!');\n        } catch (error) {\n          console.error('Error sending message:', error);\n          alert('Failed to send message. Please try again.');\n        } finally {\n          this.loading = false;\n        }\n      }\n    },\n    generateVCard() {\n      if (!this.currentUser) return '';\n      \n      return `BEGIN:VCARD\nVERSION:3.0\nFN:${this.currentUser.name}\nTITLE:${this.currentUser.title}\nTEL;TYPE=WORK,VOICE:${this.currentUser.phone}\nTEL;TYPE=WORK,WHATSAPP:${this.currentUser.whatsapp}\nEMAIL;TYPE=WORK:${this.currentUser.email}\nADR;TYPE=WORK:;;${this.currentUser.address}\nORG:Andy Casket Service\nURL:${window.location.origin}/agent\nEND:VCARD`\n    },\n    \n    async downloadVCard() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      \n      const vcard = this.generateVCard()\n      const blob = new Blob([vcard], { type: 'text/vcard' })\n      const url = window.URL.createObjectURL(blob)\n      const link = document.createElement('a')\n      link.href = url\n      link.setAttribute('download', `${this.currentUser.name}.vcf`)\n      document.body.appendChild(link)\n      link.click()\n      document.body.removeChild(link)\n    },\n\n    shareOnWhatsApp() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      \n      const text = `Contact Card - ${this.currentUser.name}\\n${window.location.href}`\n      window.open(`https://wa.me/?text=${encodeURIComponent(text)}`)\n    },\n\n    shareCard() {\n      if (!this.isAuthenticated) {\n        this.$router.push('/login');\n        return;\n      }\n      \n      this.shareDialog = true\n    },\n\n    async copyLink() {\n      try {\n        await navigator.clipboard.writeText(window.location.href)\n        alert('Link copied to clipboard!');\n        this.shareDialog = false\n      } catch (err) {\n        console.error('Failed to copy:', err)\n      }\n    },\n\n    shareByEmail() {\n      const subject = `Contact Card - ${this.currentUser.name}`\n      const body = `Here's my contact information:\\n\\n${window.location.href}`\n      window.location.href = `mailto:?subject=${encodeURIComponent(subject)}&body=${encodeURIComponent(body)}`\n      this.shareDialog = false\n    }\n  },\n  created() {\n    // 检查是否有保存的用户信息\n    const savedUser = localStorage.getItem('user');\n    if (savedUser && !this.isAuthenticated) {\n      this.$store.commit('SET_USER', JSON.parse(savedUser));\n    }\n  }\n}\n</script>\n\n<style scoped>\n.page-header {\n  position: relative;\n}\n\n.overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: linear-gradient(rgba(0,0,0,0.4), rgba(0,0,0,0.7));\n}\n\n.namecard-container {\n  position: relative;\n}\n\n.namecard-wrapper {\n  border-radius: 16px;\n  overflow: hidden;\n  box-shadow: 0 15px 30px rgba(0,0,0,0.1);\n}\n\n.profile-section {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n  background: linear-gradient(135deg, var(--v-primary-base), var(--v-secondary-base));\n}\n\n.contact-info {\n  width: 100%;\n  text-align: left;\n}\n\n.contact-form, .login-prompt {\n  background: white;\n  height: 100%;\n}\n\n.login-prompt {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.share-actions {\n  background: linear-gradient(to right, rgba(0,0,0,0.02), transparent);\n  border-top: 1px solid rgba(0,0,0,0.05);\n  display: flex;\n  justify-content: center;\n}\n\n.share-btn {\n  transition: all 0.3s ease;\n  min-width: 150px;\n}\n\n.share-btn:hover {\n  transform: translateY(-2px);\n}\n\n.profile-avatar {\n  background: rgba(255, 255, 255, 0.1);\n}\n\n@media (max-width: 960px) {\n  .namecard-container {\n    margin-top: -60px;\n  }\n  \n  .profile-section {\n    padding: 40px 20px;\n  }\n}\n\n@media (max-width: 600px) {\n  .share-actions .v-row {\n    width: 100%;\n  }\n  \n  .share-actions .v-col {\n    flex: 0 0 100%;\n    max-width: 100%;\n    padding: 4px 16px;\n  }\n  \n  .share-btn {\n    width: 100%;\n    margin: 4px 0 !important;\n  }\n}\n</style> "],"mappings":"AA+NA,SAAAA,UAAA;AAEA;EACAC,IAAA;EACAC,IAAA,EAAAA,CAAA;IACAC,KAAA;IACAC,OAAA;IACAC,WAAA;IACAC,OAAA;MACAL,IAAA;MACAM,KAAA;MACAC,KAAA;MACAC,OAAA;IACA;IACAC,SAAA,GAAAC,CAAA,MAAAA,CAAA;IACAC,UAAA,GAAAD,CAAA,MAAAA,CAAA;IACAE,UAAA,GACAF,CAAA,MAAAA,CAAA,yBACAA,CAAA,gBAAAG,IAAA,CAAAH,CAAA,2BACA;IACAI,YAAA,GAAAJ,CAAA,MAAAA,CAAA;EACA;EACAK,QAAA;IACA,GAAAhB,UAAA;EACA;EACAiB,OAAA;IACA,MAAAC,cAAA;MACA,SAAAC,KAAA,CAAAC,IAAA,CAAAC,QAAA;QACA,KAAAjB,OAAA;QACA;UACA;UACA,UAAAkB,OAAA,CAAAC,OAAA,IAAAC,UAAA,CAAAD,OAAA;UACA,KAAAJ,KAAA,CAAAC,IAAA,CAAAK,KAAA;UACAC,KAAA;QACA,SAAAC,KAAA;UACAC,OAAA,CAAAD,KAAA,2BAAAA,KAAA;UACAD,KAAA;QACA;UACA,KAAAtB,OAAA;QACA;MACA;IACA;IACAyB,cAAA;MACA,UAAAC,WAAA;MAEA;AACA;AACA,UAAAA,WAAA,CAAA7B,IAAA;AACA,aAAA6B,WAAA,CAAAC,KAAA;AACA,2BAAAD,WAAA,CAAAvB,KAAA;AACA,8BAAAuB,WAAA,CAAAE,QAAA;AACA,uBAAAF,WAAA,CAAAtB,KAAA;AACA,uBAAAsB,WAAA,CAAAG,OAAA;AACA;AACA,MAAAC,MAAA,CAAAC,QAAA,CAAAC,MAAA;AACA;IACA;IAEA,MAAAC,cAAA;MACA,UAAAC,eAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;QACA;MACA;MAEA,MAAAC,KAAA,QAAAZ,aAAA;MACA,MAAAa,IAAA,OAAAC,IAAA,EAAAF,KAAA;QAAAG,IAAA;MAAA;MACA,MAAAC,GAAA,GAAAX,MAAA,CAAAY,GAAA,CAAAC,eAAA,CAAAL,IAAA;MACA,MAAAM,IAAA,GAAAC,QAAA,CAAAC,aAAA;MACAF,IAAA,CAAAG,IAAA,GAAAN,GAAA;MACAG,IAAA,CAAAI,YAAA,qBAAAtB,WAAA,CAAA7B,IAAA;MACAgD,QAAA,CAAAI,IAAA,CAAAC,WAAA,CAAAN,IAAA;MACAA,IAAA,CAAAO,KAAA;MACAN,QAAA,CAAAI,IAAA,CAAAG,WAAA,CAAAR,IAAA;IACA;IAEAS,gBAAA;MACA,UAAAnB,eAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;QACA;MACA;MAEA,MAAAkB,IAAA,0BAAA5B,WAAA,CAAA7B,IAAA,KAAAiC,MAAA,CAAAC,QAAA,CAAAgB,IAAA;MACAjB,MAAA,CAAAyB,IAAA,wBAAAC,kBAAA,CAAAF,IAAA;IACA;IAEAG,UAAA;MACA,UAAAvB,eAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;QACA;MACA;MAEA,KAAAnC,WAAA;IACA;IAEA,MAAAyD,SAAA;MACA;QACA,MAAAC,SAAA,CAAAC,SAAA,CAAAC,SAAA,CAAA/B,MAAA,CAAAC,QAAA,CAAAgB,IAAA;QACAzB,KAAA;QACA,KAAArB,WAAA;MACA,SAAA6D,GAAA;QACAtC,OAAA,CAAAD,KAAA,oBAAAuC,GAAA;MACA;IACA;IAEAC,aAAA;MACA,MAAAC,OAAA,0BAAAtC,WAAA,CAAA7B,IAAA;MACA,MAAAoD,IAAA,wCAAAnB,MAAA,CAAAC,QAAA,CAAAgB,IAAA;MACAjB,MAAA,CAAAC,QAAA,CAAAgB,IAAA,sBAAAS,kBAAA,CAAAQ,OAAA,UAAAR,kBAAA,CAAAP,IAAA;MACA,KAAAhD,WAAA;IACA;EACA;EACAgE,QAAA;IACA;IACA,MAAAC,SAAA,GAAAC,YAAA,CAAAC,OAAA;IACA,IAAAF,SAAA,UAAAhC,eAAA;MACA,KAAAmC,MAAA,CAAAC,MAAA,aAAAC,IAAA,CAAAC,KAAA,CAAAN,SAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}