{"ast":null,"code":"export default {\n  name: 'App',\n  data: () => ({\n    user: null,\n    menuItems: [{\n      text: 'Home',\n      path: '/',\n      exact: true,\n      icon: 'mdi-home'\n    }, {\n      text: 'Packages',\n      path: '/packages',\n      exact: false,\n      icon: 'mdi-package-variant'\n    }, {\n      text: 'E-namecard',\n      path: '/agent',\n      exact: false,\n      icon: 'mdi-card-account-details'\n    }, {\n      text: 'Sample display',\n      path: '/gallery',\n      exact: false,\n      icon: 'mdi-image-multiple'\n    }],\n    quickLinks: [{\n      text: 'Home',\n      to: '/'\n    }, {\n      text: 'Our Packages',\n      to: '/packages'\n    }, {\n      text: 'Contact Agent',\n      to: '/agent'\n    }, {\n      text: 'Gallery',\n      to: '/gallery'\n    }],\n    socialLinks: [{\n      icon: 'mdi-facebook',\n      link: 'https://facebook.com',\n      class: 'facebook-btn'\n    }, {\n      icon: 'mdi-instagram',\n      link: 'https://instagram.com',\n      class: 'instagram-btn'\n    }, {\n      icon: 'mdi-whatsapp',\n      link: 'https://whatsapp.com',\n      class: 'whatsapp-btn'\n    }, {\n      icon: 'mdi-telegram',\n      link: 'https://telegram.org',\n      class: 'telegram-btn'\n    }],\n    contactInfo: [{\n      icon: 'mdi-phone',\n      color: 'accent',\n      title: '+65 8800 5559',\n      subtitle: '24/7 Hotline Available'\n    }, {\n      icon: 'mdi-email',\n      color: 'accent',\n      title: 'andycasketfuneral@gmail.com',\n      subtitle: 'Online Support'\n    }, {\n      icon: 'mdi-map-marker',\n      color: 'accent',\n      title: '29 Sin Ming Dr',\n      subtitle: 'Singapore'\n    }],\n    showContactMenu: false,\n    phoneNumber: '+6588005559',\n    // 更换为实际的电话号码\n    whatsappNumber: '+6588005559',\n    // 更换为实际的 WhatsApp 号码\n    languages: [{\n      code: 'en',\n      name: 'English'\n    }, {\n      code: 'zh',\n      name: '中文'\n    }],\n    currentLanguage: 'English' // 默认语言\n  }),\n  methods: {\n    changeLanguage(langCode) {\n      this.$i18n.locale = langCode;\n      this.currentLanguage = this.languages.find(lang => lang.code === langCode).name;\n      localStorage.setItem('language', langCode); // 保存语言选择\n    }\n  },\n  created() {\n    // 初始化语言设置\n    const savedLang = localStorage.getItem('language') || 'en';\n    this.changeLanguage(savedLang);\n  }\n};","map":{"version":3,"names":["name","data","user","menuItems","text","path","exact","icon","quickLinks","to","socialLinks","link","class","contactInfo","color","title","subtitle","showContactMenu","phoneNumber","whatsappNumber","languages","code","currentLanguage","methods","changeLanguage","langCode","$i18n","locale","find","lang","localStorage","setItem","created","savedLang","getItem"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- 顶部联系信息栏 -->\n    <v-system-bar color=\"primary\" dark height=\"36\" class=\"px-6\">\n      <v-icon small class=\"mr-2\">mdi-phone</v-icon>\n      <span class=\"text-caption\">24/7 Hotline: +65 8800 5559</span>\n      <v-spacer></v-spacer>\n      <v-icon small class=\"mr-2\">mdi-email</v-icon>\n      <span class=\"text-caption mr-4\">contact@andycasket.com</span>\n      <v-divider vertical class=\"mx-2\"></v-divider>\n      <v-btn icon small href=\"https://facebook.com\" target=\"_blank\" class=\"social-btn\">\n        <v-icon small>mdi-facebook</v-icon>\n      </v-btn>\n      <v-btn icon small href=\"https://instagram.com\" target=\"_blank\" class=\"social-btn\">\n        <v-icon small>mdi-instagram</v-icon>\n      </v-btn>\n    </v-system-bar>\n\n    <!-- 主导航栏 -->\n    <v-app-bar app color=\"white\" elevation=\"1\" height=\"90\" class=\"nav-bar\">\n      <v-container class=\"py-0 fill-height\">\n        <div class=\"d-flex align-center justify-space-between full-width\">\n          <router-link to=\"/\" class=\"text-decoration-none d-flex align-center\">\n            <v-toolbar-title class=\"company-title\">\n              <div class=\"d-flex align-center\">\n                <!-- <v-img\n                  :src=\"require('@/assets/companyTitle.jpg')\"\n                  max-height=\"50\"\n                  max-width=\"50\"\n                  contain\n                  class=\"mr-4 company-logo\"\n                ></v-img> -->\n                <div class=\"company-name-group\">\n                  <span class=\"font-weight-bold company-name\">\n                    <span class=\"company-name-first\">Andy</span>\n                    <span class=\"company-name-highlight\">Casket</span>\n                    <span class=\"company-name-last\">Services</span>\n                  </span>\n                  <span class=\"company-tagline\">Professional Funeral Services</span>\n                </div>\n              </div>\n            </v-toolbar-title>\n          </router-link>\n\n          <div class=\"d-flex align-center\">\n            <div class=\"nav-menu\">\n              <v-tabs height=\"90\" background-color=\"transparent\" slider-color=\"accent\">\n                <v-tab \n                  v-for=\"item in menuItems\" \n                  :key=\"item.path\" \n                  :to=\"item.path\" \n                  :exact=\"item.exact\"\n                  class=\"nav-link\"\n                >\n                  <v-icon class=\"mb-1\">{{ item.icon }}</v-icon>\n                  <span class=\"mt-1\">{{ item.text }}</span>\n                </v-tab>\n              </v-tabs>\n            </div>\n            \n            <v-btn\n              color=\"accent\"\n              class=\"ml-4 contact-btn\"\n              rounded\n              elevation=\"2\"\n              height=\"44\"\n              x-large\n              @click=\"showContactMenu = true\"\n            >\n              <v-icon left>mdi-phone</v-icon>\n              {{ $t('contactUs') }}\n            </v-btn>\n\n            <!-- 添加语言切换按钮 -->\n            <v-menu\n              offset-y\n              transition=\"slide-y-transition\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn\n                  color=\"primary\"\n                  class=\"ml-4 lang-btn\"\n                  text\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                  height=\"44\"\n                >\n                  <v-icon left>mdi-translate</v-icon>\n                  {{ currentLanguage }}\n                  <v-icon right>mdi-chevron-down</v-icon>\n                </v-btn>\n              </template>\n              \n              <v-list>\n                <v-list-item\n                  v-for=\"(lang, i) in languages\"\n                  :key=\"i\"\n                  @click=\"changeLanguage(lang.code)\"\n                  :class=\"{ 'primary--text': currentLanguage === lang.name }\"\n                >\n                  <v-list-item-title>{{ lang.name }}</v-list-item-title>\n                </v-list-item>\n              </v-list>\n            </v-menu>\n          </div>\n        </div>\n      </v-container>\n                  <!-- 用户未登录时显示登录按钮 -->\n                  <template v-if=\"!user\">\n              <v-btn\n                color=\"primary\"\n                class=\"ml-4 auth-btn\"\n                text\n                to=\"/login\"\n              >\n                <v-icon left>mdi-login</v-icon>\n                Login\n              </v-btn>\n            </template>\n            \n            <!-- 用户已登录时显示用户菜单 -->\n            <template v-else>\n              <v-menu\n                offset-y\n                transition=\"slide-y-transition\"\n              >\n                <template v-slot:activator=\"{ on, attrs }\">\n                  <v-btn\n                    color=\"primary\"\n                    class=\"ml-4\"\n                    text\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                  >\n                    <v-icon left>mdi-account-circle</v-icon>\n                    {{ user.name }}\n                    <v-icon right>mdi-chevron-down</v-icon>\n                  </v-btn>\n                </template>\n                \n                <v-list>\n                  <v-list-item to=\"/profile\">\n                    <v-list-item-icon>\n                      <v-icon>mdi-account-edit</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-title>Profile</v-list-item-title>\n                  </v-list-item>\n                  \n                  <v-divider></v-divider>\n                  \n                  <v-list-item @click=\"logout\">\n                    <v-list-item-icon>\n                      <v-icon>mdi-logout</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-title>Logout</v-list-item-title>\n                  </v-list-item>\n                </v-list>\n              </v-menu>\n            </template>\n    </v-app-bar>\n\n    <!-- 主内容区 -->\n    <v-main class=\"main-background\">\n      <v-container fluid class=\"pa-0\">\n        <router-view></router-view>\n      </v-container>\n    </v-main>\n\n    <!-- 页脚 -->\n    <v-footer color=\"primary\" dark class=\"footer-section pt-12 pb-4\">\n      <v-container>\n        <v-row>\n          <!-- 公司信息 -->\n          <v-col cols=\"12\" md=\"4\">\n            <div class=\"footer-brand mb-6\">\n              <h3 class=\"text-h5 font-weight-bold gold-text mb-4\">Andy Casket Service</h3>\n              <p class=\"text-body-2 grey--text text--lighten-1\">\n                Professional funeral services available 24/7. Providing dignified and compassionate care for your loved ones.\n              </p>\n            </div>\n            <div class=\"social-links mt-6\">\n              <v-btn\n                v-for=\"(social, i) in socialLinks\"\n                :key=\"i\"\n                :href=\"social.link\"\n                target=\"_blank\"\n                icon\n                class=\"mr-4 social-btn\"\n                :class=\"social.class\"\n              >\n                <v-icon>{{ social.icon }}</v-icon>\n              </v-btn>\n            </div>\n          </v-col>\n\n          <!-- 联系信息 -->\n          <v-col cols=\"12\" md=\"4\">\n            <h3 class=\"text-h6 font-weight-bold mb-6\">Contact Info</h3>\n            <v-list color=\"transparent\" dark class=\"contact-list pa-0\">\n              <v-list-item \n                v-for=\"(contact, i) in contactInfo\"\n                :key=\"i\"\n                class=\"px-0 contact-item\"\n              >\n                <v-list-item-icon class=\"mr-3\">\n                  <v-icon :color=\"contact.color\">{{ contact.icon }}</v-icon>\n                </v-list-item-icon>\n                <v-list-item-content>\n                  <v-list-item-title class=\"text-body-2\">{{ contact.title }}</v-list-item-title>\n                  <v-list-item-subtitle class=\"text-caption grey--text text--lighten-1\">\n                    {{ contact.subtitle }}\n                  </v-list-item-subtitle>\n                </v-list-item-content>\n              </v-list-item>\n            </v-list>\n          </v-col>\n\n          <!-- 快速链接 -->\n          <v-col cols=\"12\" md=\"4\">\n            <h3 class=\"text-h6 font-weight-bold mb-6\">Quick Links</h3>\n            <v-list color=\"transparent\" dark class=\"quick-links pa-0\">\n              <v-list-item \n                v-for=\"(link, i) in quickLinks\" \n                :key=\"i\" \n                :to=\"link.to\" \n                class=\"px-0 quick-link-item\"\n              >\n                <v-list-item-icon class=\"mr-3\">\n                  <v-icon small color=\"accent\">mdi-chevron-right</v-icon>\n                </v-list-item-icon>\n                <v-list-item-content>\n                  <v-list-item-title class=\"text-body-2\">{{ link.text }}</v-list-item-title>\n                </v-list-item-content>\n              </v-list-item>\n            </v-list>\n          </v-col>\n        </v-row>\n\n        <v-divider class=\"my-6 opacity-1\"></v-divider>\n        \n        <!-- 版权信息 -->\n        <v-row justify=\"space-between\" align=\"center\" class=\"copyright-section\">\n          <v-col cols=\"12\" md=\"auto\">\n            <span class=\"text-caption grey--text text--lighten-1\">\n              &copy; {{ new Date().getFullYear() }} Andy Casket Services. All Rights Reserved.\n            </span>\n          </v-col>\n          <v-col cols=\"12\" md=\"auto\" class=\"mt-4 mt-md-0\">\n            <a href=\"#\" class=\"text-caption grey--text text--lighten-1 text-decoration-none mr-4 footer-link\">Privacy Policy</a>\n            <a href=\"#\" class=\"text-caption grey--text text--lighten-1 text-decoration-none footer-link\">Terms of Service</a>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-footer>\n\n    <!-- 添加联系方式弹出菜单 -->\n    <v-dialog\n      v-model=\"showContactMenu\"\n      max-width=\"340\"\n      transition=\"dialog-bottom-transition\"\n    >\n      <v-card class=\"contact-dialog\">\n        <v-card-title class=\"headline primary white--text\">\n          Contact Methods\n          <v-spacer></v-spacer>\n          <v-btn icon dark @click=\"showContactMenu = false\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n        </v-card-title>\n\n        <v-card-text class=\"pa-6\">\n          <v-list>\n            <v-list-item\n              @click=\"makePhoneCall\"\n              class=\"contact-option mb-3\"\n              elevation=\"2\"\n            >\n              <v-list-item-icon>\n                <v-icon color=\"primary\" large>mdi-phone</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-1 font-weight-bold\">Contact Number</v-list-item-title>\n                <v-list-item-subtitle>+65 8800 5559</v-list-item-subtitle>\n              </v-list-item-content>\n            </v-list-item>\n\n            <v-list-item\n              @click=\"openWhatsApp\"\n              class=\"contact-option\"\n              elevation=\"2\"\n            >\n              <v-list-item-icon>\n                <v-icon color=\"green\" large>mdi-whatsapp</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-1 font-weight-bold\">WhatsApp Msg</v-list-item-title>\n                <v-list-item-subtitle>+65 8800 5559</v-list-item-subtitle>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  data: () => ({\n    user: null,\n    menuItems: [\n      { text: 'Home', path: '/', exact: true, icon: 'mdi-home' },\n      { text: 'Packages', path: '/packages', exact: false, icon: 'mdi-package-variant' },\n      { text: 'E-namecard', path: '/agent', exact: false, icon: 'mdi-card-account-details' },\n      { text: 'Sample display', path: '/gallery', exact: false, icon: 'mdi-image-multiple' }\n    ],\n    quickLinks: [\n      { text: 'Home', to: '/' },\n      { text: 'Our Packages', to: '/packages' },\n      { text: 'Contact Agent', to: '/agent' },\n      { text: 'Gallery', to: '/gallery' }\n    ],\n    socialLinks: [\n      { icon: 'mdi-facebook', link: 'https://facebook.com', class: 'facebook-btn' },\n      { icon: 'mdi-instagram', link: 'https://instagram.com', class: 'instagram-btn' },\n      { icon: 'mdi-whatsapp', link: 'https://whatsapp.com', class: 'whatsapp-btn' },\n      { icon: 'mdi-telegram', link: 'https://telegram.org', class: 'telegram-btn' }\n    ],\n    contactInfo: [\n      { \n        icon: 'mdi-phone',\n        color: 'accent',\n        title: '+65 8800 5559',\n        subtitle: '24/7 Hotline Available'\n      },\n      {\n        icon: 'mdi-email',\n        color: 'accent',\n        title: 'andycasketfuneral@gmail.com',\n        subtitle: 'Online Support'\n      },\n      {\n        icon: 'mdi-map-marker',\n        color: 'accent',\n        title: '29 Sin Ming Dr',\n        subtitle: 'Singapore'\n      }\n    ],\n    showContactMenu: false,\n    phoneNumber: '+6588005559', // 更换为实际的电话号码\n    whatsappNumber: '+6588005559', // 更换为实际的 WhatsApp 号码\n    languages: [\n      { code: 'en', name: 'English' },\n      { code: 'zh', name: '中文' }\n    ],\n    currentLanguage: 'English', // 默认语言\n  }),\n  methods: {\n    changeLanguage(langCode) {\n      this.$i18n.locale = langCode\n      this.currentLanguage = this.languages.find(lang => lang.code === langCode).name\n      localStorage.setItem('language', langCode) // 保存语言选择\n    }\n  },\n  created() {\n    // 初始化语言设置\n    const savedLang = localStorage.getItem('language') || 'en'\n    this.changeLanguage(savedLang)\n  }\n}\n</script>"],"mappings":"AAmTA;EACAA,IAAA;EACAC,IAAA,EAAAA,CAAA;IACAC,IAAA;IACAC,SAAA,GACA;MAAAC,IAAA;MAAAC,IAAA;MAAAC,KAAA;MAAAC,IAAA;IAAA,GACA;MAAAH,IAAA;MAAAC,IAAA;MAAAC,KAAA;MAAAC,IAAA;IAAA,GACA;MAAAH,IAAA;MAAAC,IAAA;MAAAC,KAAA;MAAAC,IAAA;IAAA,GACA;MAAAH,IAAA;MAAAC,IAAA;MAAAC,KAAA;MAAAC,IAAA;IAAA,EACA;IACAC,UAAA,GACA;MAAAJ,IAAA;MAAAK,EAAA;IAAA,GACA;MAAAL,IAAA;MAAAK,EAAA;IAAA,GACA;MAAAL,IAAA;MAAAK,EAAA;IAAA,GACA;MAAAL,IAAA;MAAAK,EAAA;IAAA,EACA;IACAC,WAAA,GACA;MAAAH,IAAA;MAAAI,IAAA;MAAAC,KAAA;IAAA,GACA;MAAAL,IAAA;MAAAI,IAAA;MAAAC,KAAA;IAAA,GACA;MAAAL,IAAA;MAAAI,IAAA;MAAAC,KAAA;IAAA,GACA;MAAAL,IAAA;MAAAI,IAAA;MAAAC,KAAA;IAAA,EACA;IACAC,WAAA,GACA;MACAN,IAAA;MACAO,KAAA;MACAC,KAAA;MACAC,QAAA;IACA,GACA;MACAT,IAAA;MACAO,KAAA;MACAC,KAAA;MACAC,QAAA;IACA,GACA;MACAT,IAAA;MACAO,KAAA;MACAC,KAAA;MACAC,QAAA;IACA,EACA;IACAC,eAAA;IACAC,WAAA;IAAA;IACAC,cAAA;IAAA;IACAC,SAAA,GACA;MAAAC,IAAA;MAAArB,IAAA;IAAA,GACA;MAAAqB,IAAA;MAAArB,IAAA;IAAA,EACA;IACAsB,eAAA;EACA;EACAC,OAAA;IACAC,eAAAC,QAAA;MACA,KAAAC,KAAA,CAAAC,MAAA,GAAAF,QAAA;MACA,KAAAH,eAAA,QAAAF,SAAA,CAAAQ,IAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAR,IAAA,KAAAI,QAAA,EAAAzB,IAAA;MACA8B,YAAA,CAAAC,OAAA,aAAAN,QAAA;IACA;EACA;EACAO,QAAA;IACA;IACA,MAAAC,SAAA,GAAAH,YAAA,CAAAI,OAAA;IACA,KAAAV,cAAA,CAAAS,SAAA;EACA;AAC